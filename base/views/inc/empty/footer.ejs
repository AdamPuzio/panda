<%
let jsFiles = locals.js || []

let cssFiles = locals.css || []
%>

    <% cssFiles.forEach(function(item){ %>
    <link href="<%-item%>" rel="stylesheet" />
    <% }) %>
    <% jsFiles.forEach(function(item){ %>
    <script type="text/javascript" src="<%-item%>"></script>
    <% }) %>
    
</body>
</html>